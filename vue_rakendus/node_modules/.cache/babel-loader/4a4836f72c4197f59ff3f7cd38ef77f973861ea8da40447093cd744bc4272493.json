{"ast":null,"code":"import { ref, onMounted /* watch */ } from 'vue';\nimport axios from 'axios';\nexport default {\n  props: ['onNewLoan'],\n  setup( /* props */\n  ) {\n    const loan = ref([]);\n    const fetchLoan = async () => {\n      try {\n        const response = await axios.post('/api/calculateEmi', {\n          amount: 50000,\n          interestRate: 6,\n          term: 3\n        });\n        loan.value = response.data.emi_breakdown.map(year => ({\n          ...year,\n          monthly_breakdown: year.monthly_breakdown.map(month => ({\n            ...month,\n            remaining_loan_amount: month.remaining_loan_amount.toFixed(2),\n            monthly_principal: month.monthly_principal.toFixed(2),\n            monthly_interest: month.monthly_interest.toFixed(2),\n            monthly_emi: month.monthly_emi.toFixed(2)\n          }))\n        }));\n        console.log('Fetched loan:', loan.value);\n      } catch (error) {\n        console.error('Failed to fetch loan:', error);\n      }\n    };\n\n    /*\r\n    //TODO: Implement this\r\n    watch(props.onNewLoan, (newLoan) => {\r\n    if (newLoan) {\r\n      loan.value.push(newLoan);\r\n    }\r\n    });\r\n       */\n\n    onMounted(fetchLoan);\n    return {\n      loan\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","props","setup","loan","fetchLoan","response","post","amount","interestRate","term","value","data","emi_breakdown","map","year","monthly_breakdown","month","remaining_loan_amount","toFixed","monthly_principal","monthly_interest","monthly_emi","console","log","error"],"sources":["C:\\Users\\37253\\Desktop\\Coop-praktika\\Maksegraafiku_rakendus\\vue_rakendus\\src\\components\\LoanTable.vue"],"sourcesContent":["<template>\r\n  <table v-if=\"loan.length > 0\">\r\n    <thead>\r\n    <tr>\r\n      <th>Aasta</th>\r\n      <th>Yearly EMI</th>\r\n      <th>Yearly Principal</th>\r\n      <th>Aastane intress</th>\r\n      <th>Kuu</th>\r\n      <th>P천hiosa makse</th>\r\n      <th>Monthly Principal</th>\r\n      <th>Intress</th>\r\n      <th>P천hiosa j채채k</th>\r\n    </tr>\r\n    </thead>\r\n    <tbody>\r\n    <tr :key=\"year.year\">\r\n      <td>{{ year.year }}</td>\r\n      <td>{{ year.yearly_emi }}</td>\r\n      <td>{{ year.yearly_principal }}</td>\r\n      <td>{{ year.yearly_interest }}</td>\r\n      <template v-for=\"month in year.monthly_breakdown\" :key=\"month.month\">\r\n        <tr>\r\n          <td>{{ month.month }}</td>\r\n          <td>{{ month.monthly_emi }}</td>\r\n          <td>{{ month.monthly_principal }}</td>\r\n          <td>{{ month.monthly_interest }}</td>\r\n          <td>{{ month.remaining_loan_amount }}</td>\r\n        </tr>\r\n      </template>\r\n    </tr>\r\n    </tbody>\r\n  </table>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport { ref, onMounted,/* watch */ } from 'vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\nprops: ['onNewLoan'],\r\nsetup(/* props */) {\r\nconst loan = ref([]);\r\n\r\n    const fetchLoan = async () => {\r\n      try {\r\n        const response = await axios.post('/api/calculateEmi', {\r\n            amount: 50000,\r\n            interestRate: 6,\r\n            term: 3\r\n        });\r\n        loan.value = response.data.emi_breakdown.map(year => ({\r\n          ...year,\r\n          monthly_breakdown: year.monthly_breakdown.map(month => ({\r\n            ...month,\r\n            remaining_loan_amount: month.remaining_loan_amount.toFixed(2),\r\n            monthly_principal: month.monthly_principal.toFixed(2),\r\n            monthly_interest: month.monthly_interest.toFixed(2),\r\n            monthly_emi: month.monthly_emi.toFixed(2),\r\n          })),\r\n        }));\r\n        console.log('Fetched loan:', loan.value)\r\n      } catch (error) {\r\n        console.error('Failed to fetch loan:', error);\r\n      }\r\n    };\r\n\r\n    /*\r\n    //TODO: Implement this\r\n    watch(props.onNewLoan, (newLoan) => {\r\n    if (newLoan) {\r\n      loan.value.push(newLoan);\r\n    }\r\n    });\r\n\r\n     */\r\n\r\n    onMounted(fetchLoan);\r\n\r\n    return {\r\n      loan,\r\n    };\r\n  },\r\n};\r\n</script>"],"mappings":"AAsCA,SAASA,GAAG,EAAEC,SAAS,CAAC,mBAAmB,KAAK;AAChD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACfC,KAAK,EAAE,CAAC,WAAW,CAAC;EACpBC,KAAKA,CAAA,CAAC;EAAA,EAAa;IACnB,MAAMC,IAAG,GAAIL,GAAG,CAAC,EAAE,CAAC;IAEhB,MAAMM,SAAQ,GAAI,MAAAA,CAAA,KAAY;MAC5B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAML,KAAK,CAACM,IAAI,CAAC,mBAAmB,EAAE;UACnDC,MAAM,EAAE,KAAK;UACbC,YAAY,EAAE,CAAC;UACfC,IAAI,EAAE;QACV,CAAC,CAAC;QACFN,IAAI,CAACO,KAAI,GAAIL,QAAQ,CAACM,IAAI,CAACC,aAAa,CAACC,GAAG,CAACC,IAAG,KAAM;UACpD,GAAGA,IAAI;UACPC,iBAAiB,EAAED,IAAI,CAACC,iBAAiB,CAACF,GAAG,CAACG,KAAI,KAAM;YACtD,GAAGA,KAAK;YACRC,qBAAqB,EAAED,KAAK,CAACC,qBAAqB,CAACC,OAAO,CAAC,CAAC,CAAC;YAC7DC,iBAAiB,EAAEH,KAAK,CAACG,iBAAiB,CAACD,OAAO,CAAC,CAAC,CAAC;YACrDE,gBAAgB,EAAEJ,KAAK,CAACI,gBAAgB,CAACF,OAAO,CAAC,CAAC,CAAC;YACnDG,WAAW,EAAEL,KAAK,CAACK,WAAW,CAACH,OAAO,CAAC,CAAC;UAC1C,CAAC,CAAC;QACJ,CAAC,CAAC,CAAC;QACHI,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEpB,IAAI,CAACO,KAAK;MACzC,EAAE,OAAOc,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF,CAAC;;IAED;;;;;;;;;IAUAzB,SAAS,CAACK,SAAS,CAAC;IAEpB,OAAO;MACLD;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
lue.startDate = 'Alguskuupäev ei tohi olla minevikus';\r\n        valid = false;\r\n      }\r\n\r\n      return valid;\r\n    };\r\n    const submitForm = () => {\r\n      if (validateForm()) {\r\n        const newLoan = {\r\n          laenusumma: loanAmount.value,\r\n          perioodKuudes: period.value,\r\n          alguskuupäev: startDate.value,\r\n        };\r\n        //emit('new-loan', newLoan);\r\n        return newLoan;\r\n        //TODO :)\r\n      }\r\n    };\r\n\r\n    return {\r\n      loanAmount,\r\n      period,\r\n      startDate,\r\n      errors,\r\n      today,\r\n      submitForm,\r\n      //emit,\r\n      //TODO :)\r\n    };\r\n  },\r\n};\r\n</script>"],"mappings":"AAsBA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AAEnC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAIH,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMI,MAAK,GAAIJ,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMK,SAAQ,GAAIL,GAAG,CAAC,EAAE,CAAC;IACzB,MAAMM,MAAK,GAAIN,GAAG,CAAC,CAAC,CAAC,CAAC;IAEtB,MAAMO,KAAI,GAAIN,QAAQ,CAAC,MAAM;MAC3B,MAAMO,IAAG,GAAI,IAAIC,IAAI,CAAC,CAAC;MACvB,IAAIC,IAAG,GAAK,GAAEF,IAAI,CAACG,WAAW,CAAC,CAAE,IAAGH,IAAI,CAACI,QAAQ,CAAC,IAAI,CAAE,IAAGJ,IAAI,CAACK,OAAO,CAAC,CAAE,EAAC;MAC3EC,OAAO,CAACC,GAAG,CAACL,IAAG,GAAI,WAAW,CAAC;MAC/B,OAAQ,GAAEF,IAAI,CAACG,WAAW,CAAC,CAAE,IAAGH,IAAI,CAACI,QAAQ,CAAC,IAAI,CAAE,IAAGJ,IAAI,CAACK,OAAO,CAAC,CAAE,EAAC;IACzE,CAAC,CAAC;IAEF,MAAMG,YAAW,GAAIA,CAAA,KAAM;MACzB,IAAIC,KAAI,GAAI,IAAI;MAChBX,MAAM,CAACY,KAAI,GAAI,CAAC,CAAC;MAEjB,IAAI,CAACf,UAAU,CAACe,KAAI,IAAKf,UAAU,CAACe,KAAI,GAAI,KAAI,IAAKf,UAAU,CAACe,KAAI,GAAI,MAAM,EAAE;QAC9EZ,MAAM,CAACY,KAAK,CAACf,UAAS,GAAI,gDAAgD;QAC1Ec,KAAI,GAAI,KAAK;MACf;MAEA,IAAI,CAACb,MAAM,CAACc,KAAI,IAAKd,MAAM,CAACc,KAAI,GAAI,CAAC,EAAE;QACrCZ,MAAM,CAACY,KAAK,CAACd,MAAK,GAAI,qCAAqC;QAC3Da,KAAI,GAAI,KAAK;MACf;MAEA,IAAIZ,SAAS,CAACa,KAAI,IAAKb,SAAS,CAACa,KAAI,GAAIX,KAAK,CAACW,KAAK,EAAE;QACpDZ,MAAM,CAACY,KAAK,CAACb,SAAQ,GAAI,qCAAqC;QAC9DY,KAAI,GAAI,KAAK;MACf;MAEA,OAAOA,KAAK;IACd,CAAC;IACD,MAAME,UAAS,GAAIA,CAAA,KAAM;MACvB,IAAIH,YAAY,CAAC,CAAC,EAAE;QAClB,MAAMI,OAA